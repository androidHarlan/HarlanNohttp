<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.harlan.lhc.harlannohttp">

    <uses-permission android:name="android.permission.CAMERA" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS" />
    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:name=".Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.harlan.lhc.harlannohttp.activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.OriginalActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.MethodActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.ImageActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.json.JsonActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.PostBodyActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.define.DefineRequestActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.cache.CacheActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.RedirectActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.upload.UploadFileActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.download.DownloadActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.cancel.CancelActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.SyncActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.ProXYActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.HttpsActivity"></activity>
        <activity android:name="com.harlan.lhc.harlannohttp.activity.cache.CacheHttpActivity"></activity>


    </application>

</manifest>
